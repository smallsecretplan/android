<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shan.splan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/BaseTheme.Cover">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".GovernmentResourcesActivity" />

        <activity android:name=".SelfAssessmentActivity" />

        <activity android:name=".StartSelfAssessmentActivity" />

        <activity android:name=".FinishSelfAssessmentActivity" />

        <activity android:name=".ChatActivity" />

        <activity android:name=".FriendHelpActivity" />

        <activity android:name=".ActionPlanActivity" />

        <activity android:name=".ArticleListActivity" />

        <activity android:name=".ChatListActivity" />

    </application>

</manifest>